<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeDismissLayout">
        <attr name="dismiss_position" format="float" />
        <attr name="swipe_enable" format="boolean" />
        <attr name="dismiss_direction" format="enum">
            <enum name="top" value="0" />
            <enum name="start" value="1" />
            <enum name="bottom" value="2" />
            <enum name="end" value="3" />
        </attr>
    </declare-styleable>
</resources>